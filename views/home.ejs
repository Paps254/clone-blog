
<%- include("partials/header.ejs")%>
<div class="container">
    <div class="card">
      <% if(posts){%>
        <% posts.forEach((post,id)=>{%>
        <div class="card-header">
          <%= posts.title%>
        </div>
        <div class="card-body">
          <blockquote class="blockquote mb-0">
            <p class="mb-0"><%= posts.body%></p>
          </blockquote>"
          <footer class="blockquote-footer"> <%= posts.created_on%><cite title="Source Title">Source Title</cite></footer>
          <a href="#" class="btn btn-primary">save</a>
        </div>
      </div>
      <%})%>
      <%}%>
</div>

<%- include("partials/footer.ejs")%>

